{
  "compilerOptions": {
    "target": "es5",
    "lib": ["dom", "dom.iterable", "esnext"],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "noEmit": true,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve"
  },
  "include": ["next-env.d.ts", "**/*.ts", "**/*.tsx", "public/images/index.js", "pages/_document.tsx", "store/cartModel.js", "store/userModel.js", "store/index.js", "store/categoryModel.js", "store/userModel.js", "store/recentViewedProducts.js"],
  "exclude": ["node_modules"]
}
